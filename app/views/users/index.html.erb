<div class="container px-5 px-sm-0">
    <div class="row">
      <!--<div class="col-md-3">-->
        <%= render 'users/list', user: @user, users: @users%>
      <!--</div>-->
      <div class="col-md-8 offset-md-1">
        <h2>Users</h2>
        <table class="table">
          <thead>
            <tr>
             <th>image</th>
             <th>name</th>
             <th colspan="3"></th>
            </tr>
          </thead>
          <% @userss.each do |user| %>
          <tbody>
            <tr>
              <td><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg", class: "attachment user profile_image fallback" %></td>
              <td><%= user.name %></td>
              <td><%= link_to "Show", user_path(user.id)%></td>
            </tr>
          </tbody>
          <% end %>
        </table>
      </div>
    </div>
</div>
